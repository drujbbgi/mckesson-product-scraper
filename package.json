{
  "name": "mckesson-product-scraper",
  "version": "1.0.1",
  "description": "Product scraper for McKesson medical supplies website",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "scrape": "node dist/index.js",
    "scrape:workers": "node dist/index.js --workers 3",
    "scrape:fast": "node dist/index.js --workers 5 --delay 1000",
    "scrape:safe": "node dist/index.js --workers 1 --delay 2000",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "scraper",
    "mckesson",
    "product",
    "medical-supplies"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "p-limit": "^5.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
